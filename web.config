<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<system.webServer>
		<handlers>
			<add name="httpPlatformHandler" 
            path="*" 
            verb="*" 
            modules="httpPlatformHandler" 
            resourceType="Unspecified"  />
		</handlers>
		<httpPlatform 
        stdoutLogEnabled="true" 
        stdoutLogFile=".\logs\stdout.log" 
        startupTimeLimit="20" 
        processPath="C:\Users\nckus\AppData\Local\Programs\Python\Python313\python.exe" 
        arguments="-m python app.py"></httpPlatform>
	</system.webServer>
</configuration>